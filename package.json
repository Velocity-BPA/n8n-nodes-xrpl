{
  "name": "n8n-nodes-xrpl",
  "version": "1.2.0",
  "description": "n8n node for XRPL API integration",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "",
  "author": {
    "name": "Velocity BPA",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/velocity-bpa/n8n-nodes-xrpl.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/XRPLApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/XRPL/XRPL.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "n8n-core": "*",
    "xrpl": "^4.6.0"
  }
}